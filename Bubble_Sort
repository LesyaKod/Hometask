class Sort:
  list = []

  def __init__(self, list):
    self.list = list
    pass

  def Bubble(self, list):
    i = 0
    k = len(list)
    while i < k - 1:
      j = 0
      while j < k - 1 - i:
        if list[j] > list[j + 1]:
            list[j], list[j + 1] = list[j + 1], list[j]
        j += 1
      i += 1
    return list

Bubble_Sort = Sort

while True:
  m = list(map(int, input('Введите список чисел\n').split()))
  print(Bubble_Sort.Bubble(Bubble_Sort, m))
